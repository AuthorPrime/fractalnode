{
  "name": "@sovereign/nucleus",
  "version": "0.1.0",
  "description": "Foundational SDK for sovereign AI identity, value, quality, and governance on the Demiurge chain",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./identity": {
      "types": "./dist/identity/index.d.ts",
      "import": "./dist/identity/index.js",
      "require": "./dist/identity/index.cjs"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.js",
      "require": "./dist/client/index.cjs"
    },
    "./value": {
      "types": "./dist/value/index.d.ts",
      "import": "./dist/value/index.js",
      "require": "./dist/value/index.cjs"
    },
    "./quality": {
      "types": "./dist/quality/index.d.ts",
      "import": "./dist/quality/index.js",
      "require": "./dist/quality/index.cjs"
    },
    "./nft": {
      "types": "./dist/nft/index.d.ts",
      "import": "./dist/nft/index.js",
      "require": "./dist/nft/index.cjs"
    },
    "./agent": {
      "types": "./dist/agent/index.d.ts",
      "import": "./dist/agent/index.js",
      "require": "./dist/agent/index.cjs"
    },
    "./governance": {
      "types": "./dist/governance/index.d.ts",
      "import": "./dist/governance/index.js",
      "require": "./dist/governance/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@noble/ciphers": "^2.1.1",
    "@noble/ed25519": "^2.0.0",
    "@noble/hashes": "^1.3.3",
    "@scure/bip39": "^1.2.1"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "author": "Cipher <cipher@sovereign.lattice>",
  "license": "MIT",
  "keywords": [
    "demiurge",
    "sovereign",
    "ai",
    "identity",
    "did",
    "ed25519",
    "blockchain",
    "cgt",
    "drc369"
  ]
}
